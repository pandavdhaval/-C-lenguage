


#include <iostream>
using namespace std;

class addition
{
 protected:
  
  int a=10;
  int b=20;
  void methord()
  {
      cout<<"fast number :"<<a;
      cout<<"second number :"<<b;
  }
 
};

class addition1: public addition
{
    public:
    
    void methord1()
    {
        cout<<"addition :"<<a+b<<endl;
        cout<<"multiflication :"<<a*b;
    }
};


int main()
{
    addition1 obj;
   
    obj.methord1();
    
    return 0;
}