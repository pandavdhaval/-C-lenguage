

#include <iostream>
using namespace std;

int main()
{
    char ch;
    
    cout<<"enter ch :";
    cin>>ch;
    
    if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
    {
        cout<<"this is vowel char "<<ch;
    }
    else 
    {
        cout<<"this is consonet  char ";
    }

    return 0;
}