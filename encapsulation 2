

#include <iostream>
using namespace std;

class number
{
    private:
    
    int a;
    
    public:
    
    void setSalary(int b)
    {
        cout<<"\nbefore"<<a;
        
        a=b;
        
        cout<<"\nafter"<<a;
    }
    
    int getSalary()
    {
        return a;
    }
    
    
};


int main()
{
    number obj1;
    
    obj1.setSalary(200);
    
    
    int c=obj1.getSalary();
    
    cout<<"\nnew number "<<c;
    
    return 0;
}