




  #include <iostream>
  using namespace std;
  
  int main()
  {
      int a,b;
      
      cout<<"enter number :";
      cin>>a;
      
      cout<<"enter number :";
      cin>>b;
      
      cout<<" addition number :"<<a+b;
      
      
      
      
      return 0;
  }
  
