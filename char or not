

#include <iostream>
using namespace std;

int main()
{
    char ch;
    
    cout<<"enter char :";
    cin>>ch;
    
    if((ch='A'||ch<='Z')||(ch='a'||ch<='z'))
    {
        cout<<"this is char ";
    }
    else
    {
        cout<<"this is not char ";
    }
    
    
    
    
    
    return 0;
}

